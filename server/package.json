{
  "name": "admin-api",
  "version": "1.0.0",
  "description": "Admin Panel API Server",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:dev": "tsx watch src/index.ts",
    "seed": "tsx src/scripts/seed.ts",
    "seed:products": "tsx src/scripts/seedProducts.ts",
    "seed:admins": "tsx src/scripts/seedAdmins.ts",
    "lint": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.{ts,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,json}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepare": "husky install",
    "dev:restaurant": "PORT=5002 tsx watch src/products/restaurant/server.ts",
    "dev:livenotes": "PORT=5003 tsx watch src/products/livenotes/server.ts",
    "dev:rentalcabbooking": "PORT=5004 tsx watch src/products/rentalcabbooking/server.ts",
    "dev:whatsappapi": "PORT=5005 tsx watch src/products/whatsappapi/server.ts",
    "dev:crm": "PORT=5006 tsx watch src/products/crm/server.ts",
    "start:restaurant": "PORT=5002 node dist/products/restaurant/server.js",
    "start:livenotes": "PORT=5003 node dist/products/livenotes/server.js",
    "start:rentalcabbooking": "PORT=5004 node dist/products/rentalcabbooking/server.js",
    "start:whatsappapi": "PORT=5005 node dist/products/whatsappapi/server.js",
    "start:crm": "PORT=5006 node dist/products/crm/server.js"
  },
  "keywords": [
    "admin",
    "api",
    "express",
    "mongodb"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@bull-board/express": "^5.14.0",
    "aws-sdk": "^2.1544.0",
    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "bull": "^4.12.0",
    "cloudinary": "^1.41.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "google-auth-library": "^10.5.0",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mongoose": "^8.0.3",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^5.0.4",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "response-time": "^2.3.2",
    "slugify": "^1.6.6",
    "socket.io": "^4.7.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1",
    "validator": "^13.11.0",
    "yamljs": "^0.3.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/bull": "^4.10.0",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/hpp": "^0.2.3",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.202",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.6",
    "@types/response-time": "^2.3.9",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.7",
    "@types/validator": "^13.11.7",
    "@types/yamljs": "^0.2.34",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.0.2",
    "prettier": "^3.2.5",
    "prom-client": "^15.1.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
